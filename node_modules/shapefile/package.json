{
  "name": "shapefile",
  "version": "0.6.5",
  "description": "An implementation of the shapefile (.shp) spatial data format.",
  "keywords": [
    "geojson",
    "shapefile"
  ],
  "homepage": "https://github.com/mbostock/shapefile",
  "license": "BSD-3-Clause",
